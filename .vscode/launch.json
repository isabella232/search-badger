{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "program": "${workspaceFolder}/out/main.js",
      "runtimeArgs": ["-r", "source-map-support/register"],
      "sourceMaps": true,
      "outFiles": ["out/**/*.js"],
      "env": {
        "PORT": "8090"
      }
    }
  ]
}
